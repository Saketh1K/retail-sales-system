{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "sales_demo.db"
                ]
            }
        }
    ],
    "env": {
        "DB_FILE_NAME": "sales_demo.db"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}